<h1>Index of All Users</h1>

<% @users.each do |user| %>
  <p>Name: <%= user.username %>, <br>Email: <%= link_to user.email, "mailto: #{user.email}" %>
  <% if user.admin? %><strong><p>Admin <% end %> </strong></p>
  <p><%= link_to "Delete", admin_user_path(user), method: :delete %></p>
  <hr>
<% end %>



<%= link_to "Back to my admin page!", admin_user_path(current_user) %>
